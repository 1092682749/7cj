/* pages/mine/mine.wxss */
page{
  background-color: rgb(244, 244, 244);
}
.mine-head{
  width: 100%;
  height: 340rpx;
  background-color: white;
  display: flex;
  justify-content: center;
  align-items: center;
}
.mine-head view image{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
}
.mine-head view{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.mine-head view text{
  margin-top: 40rpx;
  font-size: 28rpx;
  padding: 0;
}
.mine-list{
  margin-top: 30rpx;
}